<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/blurimageb"
    android:layout_height="match_parent"
    tools:context=".MainActivity7">

    <EditText
        android:id="@+id/e11"
        android:layout_width="175dp"
        android:layout_height="71dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:ems="10"
        android:hint="enter your name"
        android:inputType="textPersonName"
        android:textColorHint="#fff" />

    <Button
        android:id="@+id/button5"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="99dp"
        android:background="#b9efbe"
        android:text="show" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="322dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="207dp"
        android:hint="your phone number"
        android:textColorHint="#fff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="323dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="157dp"
        android:hint="your address "
        android:textColorHint="#fff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t4"
        android:layout_width="323dp"
        android:layout_height="40dp"

        android:layout_alignParentStart="true"
        android:layout_below="@+id/t3"
        android:hint="your date of birth"
        android:textColorHint="#fff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t5"
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="142dp"
        android:hint="your email address"
        android:textColorHint="#fff"
        android:textSize="20dp" />


    <TextView
        android:id="@+id/t6"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/t5"
        android:layout_marginTop="-142dp"
        android:hint="male or female"
        android:textColorHint="#fff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="48dp"
        android:hint="12%"
        android:textColorHint="#fff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t8"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/t6"
        android:hint="10th%"
        android:textColorHint="#fff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/t6"
        android:layout_alignTop="@+id/t7"
        android:textColorHint="#fff"
        android:textSize="20dp" />


    <TextView
        android:id="@+id/t9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="14dp"
        android:hint="btech aggregate"
        android:textColorHint="#fff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/t6"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="82dp"
        android:hint="branch "
        android:textColorHint="#fff"
        android:textSize="20dp" />


    <ImageView
        android:id="@+id/image1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="18dp"
        android:background="#f1f0f0" />

</RelativeLayout>